{
    "name": "codicastudio/honeypot",
    "description": "A noobs worthless garbage, just learning.",
    "keywords": [
        "worthless garabage by a noob."
    ],
    "homepage": "https://github.com/codicastudio/honeypot",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael J Brown",
            "email": "codicastudiodev@gmail.com",
            "homepage": "https://codicastudio.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4.8",
        "illuminate/contracts": "^7.0",
        "illuminate/encryption": "^7.0",
        "illuminate/http": "^7.0",
        "illuminate/support": "^7.0",
        "illuminate/validation": "^7.0",
        "nesbot/carbon": "^2.0"

    },
    "require-dev": {
        "spatie/phpunit-snapshot-assertions": "^3.0",
        "orchestra/testbench": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "codicastudio\\Honeypot\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "codicastudio\\Honeypot\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "codicastudio\\Honeypot\\HoneypotServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
